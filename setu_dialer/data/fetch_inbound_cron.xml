        <record id="ir_cron_deactivate_leads" model="ir.cron">
            <field name="name">Deactivate Lead Lists at 10PM</field>
            <field name="model_id" ref="model_lead_list_data"/>
            <field name="state">code</field>
            <field name="code">model.cron_deactivate_leads()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active">True</field>
            <field name="nextcall" eval="(DateTime.now().replace(hour=21, minute=0, second=0)).strftime('%Y-%m-%d %H:%M:%S')"/>
        </record>

    </data>
