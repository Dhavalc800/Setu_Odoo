<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="agreement_legal.BinaryFieldPreview" t-inherit="web.BinaryField" t-inherit-mode="extension" owl="1">
        <xpath expr="//button[hasclass('o_clear_file_button')]" position="after">
            <button
                class="btn btn-secondary fa fa-eye"
                data-tooltip="Preview"
                aria-label="Preview"
                t-on-click="() => this.PreviewFile({})"
                        />
        </xpath>
    </t>
</templates>
