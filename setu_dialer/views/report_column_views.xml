<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="view_report_column_tree" model="ir.ui.view">
        <field name="name">report.column.tree</field>
        <field name="model">report.column</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="view_report_column_form" model="ir.ui.view">
        <field name="name">report.column.form</field>
        <field name="model">report.column</field>
        <field name="arch" type="xml">
            <form string="Dispo Name">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_report_column" model="ir.actions.act_window">
        <field name="name">CSV Column</field>
        <field name="res_model">report.column</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>