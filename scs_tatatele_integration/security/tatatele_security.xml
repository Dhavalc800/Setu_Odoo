<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="0">
            <!-- Module Category TATA Tele Service -->
        <record id="module_category_scs_tatatele_integration" model="ir.module.category">
            <field name="name">Lead Data Store</field>
            <field name="description">Helps you handle your Tata tele services security access rights</field>
            <field name="sequence">30</field>
        </record>

        <!-- TATA Tele Service User Group -->
        <record id="group_tatatele_user" model="res.groups">
            <field name="name">User</field>
            <field name="category_id" ref="scs_tatatele_integration.module_category_scs_tatatele_integration"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>

        <!-- TATA Tele Service Admin Group -->
        <record id="group_tatatele_admin" model="res.groups">
            <field name="name">Administrator</field>
            <field name="category_id" ref="scs_tatatele_integration.module_category_scs_tatatele_integration"/>
            <field name="implied_ids" eval="[(4, ref('scs_tatatele_integration.group_tatatele_user'))]"/>
        </record>
    </data>
</odoo>
