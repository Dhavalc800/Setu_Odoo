<odoo>
    <record id="csv_lead_status_tree" model="ir.ui.view">
        <field name="name">csv.lead.status.tree</field>
        <field name="model">csv.lead.status</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="is_reschedule"/>
                <field name="is_cancel"/>
            </tree>
        </field>
    </record>

    <record id="csv_lead_status_form" model="ir.ui.view">
        <field name="name">csv.lead.status.form</field>
        <field name="model">csv.lead.status</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="is_reschedule"/>
                        <field name="is_cancel"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_csv_lead_status" model="ir.actions.act_window">
        <field name="name">Csv Lead Status</field>
        <field name="res_model">csv.lead.status</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                No CSV Lead Status Found!
            </p>
        </field>
    </record>

</odoo>
