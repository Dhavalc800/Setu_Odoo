<odoo>
	<record model="ir.ui.view" id="timesheet_type_form">
		<field name="name">timesheet.type.form</field>
		<field name="model">timesheet.type</field>
		<field name="arch" type="xml">
			<form>
				<sheet>
					<group col="4" colspan="4">
						<field name="name" required="1"/>
						<field name="code"/>
						<field name="is_default"/>
					</group>
				</sheet>
			</form>
		</field>
	</record>

	<record model='ir.ui.view' id='timesheet_type_tree'>
		<field name='name'>timesheet.type.tree</field>
		<field name='model'>timesheet.type</field>
		<field name='arch' type='xml'>
			<tree>
				<field name='name'/>
				<field name="code"/>
				<field name="is_default"/>
			</tree>
		</field>
	</record>

	<record id="action_timesheet_type" model="ir.actions.act_window">
		<field name="name">Timesheet Type</field>
		<field name="res_model">timesheet.type</field>
		<field name="view_mode">tree,form</field>
	</record>
	<menuitem id="timesheet_type_menu" name="Type" 
		parent="hr_timesheet.hr_timesheet_menu_configuration" 
		action="action_timesheet_type"/>

 	
</odoo>
